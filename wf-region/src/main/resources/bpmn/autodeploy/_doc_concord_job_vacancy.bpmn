    <scriptTask id="scripttask2" name="Step_2" scriptFormat="javascript" activiti:autoStoreVariables="false">
      <script>//1 получение ИД текущего процесса
var processInstanceId = execution.getProcessInstanceId();
execution.setVariable('processInstanceId', processInstanceId)

//2 получение ИД БП
var executeProcessDefinitionId = execution.getProcessDefinitionId();
var splittedProcessDefinitionId = executeProcessDefinitionId.split(':')[0];
execution.setVariable('processDefinitionId', splittedProcessDefinitionId)

//переключение степа
var snID_Process_Activiti = execution.getProcessInstanceId()
var sKey_Step= 'step_2'
var oResultSetDoc =
documentStepService.setDocumentStep(snID_Process_Activiti, sKey_Step)

//Клонування прав для HR
var sKey_GroupPostfix = '_default_Conkord_manager_HR'
var sKey_GroupPostfix_New ='Conkord_manager_HR'
var oResult = documentStepService.cloneDocumentStepSubject(snID_Process_Activiti,
sKey_GroupPostfix, sKey_GroupPostfix_New, sKey_Step)
</script>
    </scriptTask>
